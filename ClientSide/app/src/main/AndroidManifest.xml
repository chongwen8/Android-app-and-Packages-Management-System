        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.chongwen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.chongwen.HomeActivity">

        </activity>

        <activity android:name="com.example.chongwen.zbar.CaptureActivity" />
    </application>

</manifest>